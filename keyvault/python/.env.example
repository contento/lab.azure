# Azure Key Vault Configuration
# Copy this file to .env and update with your values

# Set to 'true' to use Azure Key Vault, 'false' for local development mode
USE_AZURE=false

# Required when USE_AZURE=true
KEYVAULT_NAME=your-keyvault-name

# Local development secrets (when USE_AZURE=false)
TEST_USERNAME=alice
TEST_PASSWORD=opensesame

# Optional: Override default secret names
# USERNAME_SECRET_NAME=rosina-username-dev
# PASSWORD_SECRET_NAME=rosina-password-dev
