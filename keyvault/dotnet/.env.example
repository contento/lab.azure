# Azure Key Vault Configuration
# Copy this file to .env and update with your values
# Note: Environment variables override appsettings.json

# Key Vault URL (double underscore __ replaces : in environment variables)
KeyVault__VaultUrl=https://your-keyvault-name.vault.azure.net/

# Optional: Local development mode
# USE_LOCAL=false
# LOCAL_USERNAME=alice
# LOCAL_PASSWORD=opensesame

# Optional: Service Principal authentication (if not using Azure CLI)
# AZURE_CLIENT_ID=your-client-id
# AZURE_TENANT_ID=your-tenant-id
# AZURE_CLIENT_SECRET=your-client-secret
